{
  "api": {
    "graphqlSwipeGuide": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "apiKeyExpirationDays": 365,
              "description": ""
            }
          },
          "additionalAuthenticationProviders": []
        }
      }
    },
    "skillsapi": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "skillsfunction",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    },
    "miscapi": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "miscfunction",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    },
    "certificationapi": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "certificationfunction",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    },
    "todosApi": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "todosFunction",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "function": {
    "skillsfunction": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "miscfunction": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "certificationfunction": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "certification",
          "attributes": [
            "Name",
            "Arn"
          ],
          "attributeEnvMap": {
            "Name": "TABLE_NAME",
            "Arn": "TABLE_ARN"
          }
        }
      ]
    },
    "todosFunction": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "todosTable",
          "attributes": [
            "Name",
            "Arn"
          ],
          "attributeEnvMap": {
            "Name": "TABLE_NAME",
            "Arn": "TABLE_ARN"
          }
        }
      ]
    }
  },
  "storage": {
    "certification": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    },
    "todosTable": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  }
}